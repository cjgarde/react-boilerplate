{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "private": false,
  "description": "SASS React.js and Webpack project boilerplate. Including jest, babel and eslint.",
  "main": "index.js",
  "engines": {
    "node": ">= 10.15",
    "npm": ">= 6.5"
  },
  "scripts": {
    "build:prod": "node --max_old_space_size=2048 node_modules/.bin/webpack -p --config config/webpack.prod.js",
    "build:dev": "webpack-dev-server --display-error-details --open --config config/webpack.dev.js --hot --inline",
    "tests:unit": "jest --setupTestFrameworkScriptFile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjgarde/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "sass",
    "webpack",
    "babel",
    "eslint",
    "javascript",
    "jsx",
    "jest"
  ],
  "author": {
    "name": "Charly",
    "url": "https://github.com/cjgarde",
    "email": "devcharly@pm.me"
  },
  "homepage": "https://github.com/cjgarde",
  "license": "GPL-3.0",
  "jest": {
    "cacheDirectory": "<rootDir>/__tests__/!tmp/cache",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**"
    ],
    "coverageDirectory": "<rootDir>/__tests__/!reports",
    "coverageThreshold": {
      "./src/base-components/": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "./src/state/": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "./src/helpers/": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      },
      "./src/views/": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^views(.*)$": "<rootDir>/src/views/$1",
      "^base_components(.*)$": "<rootDir>/src/base-components/$1",
      "^state(.*)$": "<rootDir>/src$1/state",
      "^constants(.*)$": "<rootDir>/src/constants/$1",
      "^helpers(.*)$": "<rootDir>/src/helpers/$1",
      "^test_helpers(.*)$": "<rootDir>/__tests__/utils/utils.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__tests__/mocks/file-mock.js",
      "\\.(svg)$": "<rootDir>/src/__tests__/mocks/svg-mock.js",
      "\\.(css|scss)$": "<rootDir>/__tests__/mocks/file-mock.js"
    },
    "notify": true,
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/__tests__/",
      "<rootDir>/static/",
      "<rootDir>/src/index.js",
      "<rootDir>/src/styles/"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest"
    },
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "verbose": true,
    "setupFiles": [
      "<rootDir>/__tests__/mocks/shims.js",
      "<rootDir>/__tests__/setup-tests.js"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^2.0.1",
    "css-loader": "^2.1.1",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react-hot-loader": "^4.8.4",
    "redux-devtools-extension": "^2.13.8",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^2.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.3",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-bundle-analyzer": "^3.3.2",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "immutable": "^4.0.0-rc.12",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  }
}
